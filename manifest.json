{
  "manifest_version": 2,

  "name": "Meta Nav",
  "description": "Provides keyboard navigation based on <link rel=prev/next> using Alt-[ and Alt-]",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content-script.js"]
    }
  ],

  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
